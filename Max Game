#include <iostream>
#include <cmath>
using namespace std;


void fastIO(void) {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
}


int main() {
    fastIO();
    int q;
    cin>>q;
    while(q--){
        int n;
        cin>>n;
        int i=0;
        for( ; ; ){
            if(n<pow(2,i)) break;
            i++;
        }
        i--;
        cout<<pow(2,i)<<'\n';
    }
    return 0;
}
